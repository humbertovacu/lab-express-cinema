{{#with singleMovie}}
    <h1>{{title}}</h1>
    <img src="{{image}}" alt="movie">
    <h2>By: {{director}}</h2>
    <h2>Stars: </h2>
    <ul>
        {{#each stars}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    <h2>Description:</h2>
    <p>{{description}}<p>
    {{#each showtimes}}<p>{{this}}|</p>{{/each}}
{{/with}}